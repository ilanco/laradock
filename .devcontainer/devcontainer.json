{
  "name": "Laradock",
  "dockerComposeFile": "../docker-compose.yml",
  "runArgs": ["--env-file", "../.env"],
  "remoteUser": "laradock",
  "runServices": [
    "nginx",
    "postgres",
    "pgadmin"
  ],
  "service": "workspace",
  "workspaceFolder": "/var/www",
  "shutdownAction": "stopCompose",
  "postCreateCommand": "uname -a"
}
